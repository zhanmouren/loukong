<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.koron.inwlms.mapper.zoneLoss.VirtualZoneMapper">
	<select id="queryVirtualZoneList" parameterType="com.koron.inwlms.bean.DTO.zoneLoss.QueryVSZoneListDTO" resultType="com.koron.inwlms.bean.VO.zoneLoss.VirtualZoneVO">
		select a."masCode" as baseRegion, a."pCode" as zoneNo,a.name
		from gis_exist_vzone as a
		where a."virtualZoneType" = #{virtualZoneType}
	</select>
	

</mapper>