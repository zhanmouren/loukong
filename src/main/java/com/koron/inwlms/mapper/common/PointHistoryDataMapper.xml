<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.koron.inwlms.mapper.common.PointHistoryDataMapper">

  <select id="queryPointMessageByName" resultType="com.koron.inwlms.bean.VO.common.GisScadaStation" parameterType="com.koron.inwlms.bean.DTO.common.PointParamDTO">
  select * from gis_scada_station 
  <trim prefix="where" prefixOverrides="and|or">
   <if test="name != null and name != '' "> and name like '%'||#{name}||'%'</if>
   <if test="code != null and code != '' "> and p_code like '%'||#{code}||'%'</if>
  </trim>
  </select>

</mapper>