<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.koron.inwlms.mapper.common.GisMapper">
	<select id="queryZoneNosByRank" parameterType="java.lang.String" 
	resultType="com.koron.inwlms.bean.VO.apparentLoss.ZoneInfo">
	select p_code as zoneNo, name as zoneName from gis_exist_zone
	where rank = #{rank}
	</select>
	
	<select id="queryZoneNameByNo" parameterType="java.lang.String" 
	resultType="java.lang.String">
	select name from gis_exist_zone where p_code = #{zoneNo}
	</select>
	
</mapper>